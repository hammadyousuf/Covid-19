<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kashsoftdevelopment.coronavirustracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".kashmirhelp"
            android:label="KashmirHelp Line">

        </activity>
        <activity android:name=".helplines" />
        <activity
            android:name=".About"
            android:label="About">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.About" />
        </activity>
        <activity
            android:name=".Helpline"
            android:label="Helpline">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.Helpline" />
        </activity>
        <activity
            android:name=".Updates"
            android:label="All Updates">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.Updates" />
        </activity>
        <activity
            android:name=".MythsBust"
            android:label="Facts and Myths">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.MythsBust" />
        </activity>
        <activity
            android:name=".Symptoms"
            android:label="Symptoms">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.Symptoms" />
        </activity>
        <activity
            android:name=".Covid"
            android:label="Covid-19">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.Covid" />
        </activity>
        <activity
            android:name=".StatesActivity"
            android:label="State of India">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.StatesActivity" />
        </activity>
        <activity
            android:name=".AllCountries"
            android:label="All Countries">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kashsoftdevelopment.coronavirustracker.AllCountries" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/BlueActivityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>